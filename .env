# OpenSIPS AI Voice Connector - Environment Configuration
# Test ortami icin localhost ayarlari

# ===== AI Services (Development - Localhost) =====
VOSK_WEBSOCKET_URL=ws://localhost:8765
LLAMA_WEBSOCKET_URL=ws://localhost:8766
LLAMA_MODEL=llama3.2-turkish
PIPER_WEBSOCKET_URL=ws://localhost:8000/tts

# ===== VAD Configuration (Turkish Optimized) =====
VAD_CONFIDENCE=0.2
VAD_START_SECS=0.05
VAD_STOP_SECS=0.4
VAD_MIN_VOLUME=0.1
VAD_SAMPLE_RATE=16000

# ===== RTP Configuration =====
RTP_MIN_PORT=35000
RTP_MAX_PORT=35100
RTP_LOCAL_IP=192.168.1.120

# ===== SIP Listener =====
SIP_HOST=0.0.0.0
SIP_PORT=8089

# ===== System Configuration =====
LOG_LEVEL=DEBUG
ENABLE_METRICS=true
DEBUG_MODE=true

# ===== Conversation AI =====
SYSTEM_MESSAGE=Sen yardimci bir AI asistanisin. Turkce konus ve kisa, net cevaplar ver. Sesli gorusmede oldugumu unutma.
WELCOME_MESSAGE=Arayani Turkce selamla ve nasil yardimci olabilecegini sor.
LLM_MAX_TOKENS=150
LLM_TEMPERATURE=0.7
